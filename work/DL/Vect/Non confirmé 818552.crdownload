    {
      "cell_type": "code",
      "source": [
        "# gradient descent\n",
        "def gradients(A, X, y):\n",
        "  dw = 1/len(y) * np.dot(X.T, A-y)\n",
        "  db = 1/len(y) * np.sum(A-y)\n",
        "  return dw, db"
      ],
      "metadata": {
        "id": "Jv2CiG8281Do"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "dw, db = gradients(A, X, y)\n",
        "\n",
        "dw.shape, db.shape"
      ],
      "metadata": {
