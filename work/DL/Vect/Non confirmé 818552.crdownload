    {
      "cell_type": "code",
      "source": [
        "# model\n",
        "def model(X, W, b):\n",
        "  Z = X.dot(W) + b\n",
        "  A = 1 / (1 + np.exp(-Z))\n",
        "\n",
        "  return A\n"
      ],
